{
  "parser" : "babel-eslint",
  "extends": "airbnb/base",
  "ecmaFeatures": {
    "generators": true,
    "objectLiteralShorthandMethods": true,
    "arrowFunctions": true,
    "modules": true
  },
  "globals": {
    "describe": true,
    "it": true,
    "before": true,
    "beforeEach": true,
    "after": true,
    "afterEach": true
  },
  "plugins": [
    "babel"
  ],
  "rules": {
    "no-useless-concat": 2,
    "babel/generator-star-spacing": 1,
    "no-param-reassign": 0,
    "new-cap": 0,
    "no-console": 2,
    "no-unneeded-ternary": 2,
    "no-underscore-dangle": 0,
    "strict": 0
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
